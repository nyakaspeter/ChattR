{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "node"
    ],
    "rules": {
        "node/no-unsupported-features/es-syntax": 0
    },
    "settings": {}
}