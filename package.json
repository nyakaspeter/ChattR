{
  "name": "webrtc-videoconference-server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "yarn --cwd client install && yarn --cwd client build && yarn server",
    "server": "node index.js",
    "client": "yarn --cwd client start",
    "dev": "concurrently \"yarn devserver\" \"yarn devclient\"",
    "devserver": "nodemon index.js",
    "devclient": "yarn --cwd client dev"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.22.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mongoose": "^6.0.8",
    "multer": "^1.4.3",
    "multer-gridfs-storage": "^5.0.2",
    "openvidu-node-client": "^2.20.1",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.2",
    "nodemon": "^2.0.13"
  }
}
